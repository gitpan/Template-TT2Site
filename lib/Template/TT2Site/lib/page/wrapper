[% # $Id: wrapper,v 1.3 2005/01/05 18:00:02 jv Exp $ -%]
[%

SWITCH page.type;
    CASE "text";
	content;

    CASE "html";
	content WRAPPER page/html.html
		      + page/layout.html;

    CASE "nolayout";
	content WRAPPER page/html.html;

    CASE;
	THROW page.type "Invalid page type: $page.type";
END;

-%]
